; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "GQLClient",
"language": "GDScript",
"path": "res://addons/GodotGraphQL/src/gql_client.gd"
}, {
"base": "Reference",
"class": "GQLQuery",
"language": "GDScript",
"path": "res://addons/GodotGraphQL/src/gql_query.gd"
}, {
"base": "Node",
"class": "GQLQueryExecuter",
"language": "GDScript",
"path": "res://addons/GodotGraphQL/src/gql_query_executer.gd"
} ]
_global_script_class_icons={
"GQLClient": "",
"GQLQuery": "",
"GQLQueryExecuter": ""
}

[application]

config/name="godot-enjin-api"
run/main_scene="res://demo/demo.tscn"
config/icon="res://icon.png"

[autoload]

GraphQL="*res://addons/GodotGraphQL/src/gql_client.gd"
Enjin="*res://addons/godot-enjin-api/scripts/enjin_api.gd"
Secret="*res://demo/secret.gd"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
